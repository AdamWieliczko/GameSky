<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActivityFinalizeShoppingCart" android:backgroundTint="#605E9C" android:background="#558B9C"
        android:id="@+id/ListFinalizeContainer">

    <androidx.fragment.app.FragmentContainerView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:name="pl.edu.uj.wieliczko.shopapplication.fragments.BackButtonFragment"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/BackFinalizeContainer"/>
    <androidx.fragment.app.FragmentContainerView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:name="pl.edu.uj.wieliczko.shopapplication.fragments.AcceptTransactionButtonFragment"
            android:id="@+id/FinalizeButtonFinzalizeContainer"
            android:layout_marginEnd="30dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="30dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>